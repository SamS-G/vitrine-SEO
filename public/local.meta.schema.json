{
    "@context": "https://schema.org",
    "@type": "Service",
    "serviceType": "Pose de {$type}",
    "provider": {
        "@type": "LocalBusiness",
        "name": "JD Travaux Services",
        "url": "{/pose-{$type}-{$city['name']}}",
        "telephone": "+33 6 89 44 28 15",
        "address": {
            "@type": "PostalAddress",
            "addressLocality": "{$city['name']}",
            "postalCode": "{$city['postalCode']}",
            "addressCountry": "FR"
        }
    },
    "areaServed": {
        "@type": "Place",
        "name": "{$city['name']}",
        "geo": {
            "@type": "GeoCoordinates",
            "latitude": "{$city['latitude']}",
            "longitude": "{$city['longitude']}"
        }
    },
    "description": "Pose de {$type} sur-mesure à {$city['name']}. JD Travaux Services intervient rapidement pour tous vos besoins de menuiserie (volets, portes, portails, fenêtres), pose revêtements de sol, dépannage."
}
